<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>   	
    
      <!--  inherit css based theme
      <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->

	  <inherits name="com.google.gwt.http.HTTP"/>
	  <inherits name="com.google.gwt.i18n.I18N"/>
	  <inherits name="com.google.gwt.widgetideas.GlassPanel"/>	  
  	  <inherits name="com.smartgwt.SmartGwtNoScript"/>
  	  <inherits name="com.xpn.xwiki.gwt.api.Api"/>
  	  <inherits name="org.xwiki.gwt.dom.DOM"/>
  	  <inherits name="com.xpn.xwiki.wysiwyg.Wysiwyg"/>

      <!-- Specify the app entry point class.                         -->
      <entry-point class='org.mandubian.xwiki.selector.client.XWikiSelector'/>
    
      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='stylesheets/XWikiSelector.css' />

</module>
